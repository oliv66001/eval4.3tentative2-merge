      <a href="{{ post.url | url }}">{{ post.data.title }}<h2>Photo bébé</h2>{% endfor %}<img class="img-fluid galerie" src="{{ '/images/CharlesCantinphotobébénoiretblanc.jpg' | url }}" alt="Voir le book photo de Bébé"></a>
    </div>
  </div>
  <div class="row">
    <div class="col">
  {% for post in collections.grossesse %}
